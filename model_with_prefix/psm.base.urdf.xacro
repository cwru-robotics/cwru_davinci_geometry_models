<?xml version="1.0" encoding="utf-8"?>
<!--wsn variation on psm.base.urdf.xacro to add actuators for gazebo simulation...first 4 joints only-->
<!--NOTE: inertial parameters assigned are not to be believed!	merely assigned so gazebo will run -->
<!-- wsn added damping to all joints, for convenience of control and start-up-->
<robot name="dvrk_psm" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:macro name="psm" params="prefix parent_link xyz rpy color">

		<!-- fixed joint -->
		<joint name="${prefix}fixed_joint" type="fixed">
			<parent link="${parent_link}"/>
			<child link="${prefix}psm_base_link"/>
			<origin rpy="${rpy}" xyz="${xyz}"/>
		</joint>

		<!-- link 0: psm base link -->
		<link name="${prefix}psm_base_link">
			<visual>
				<origin rpy="1.5708 0 3.1416" xyz="0.039 -0.40788 -0.07879"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/psm_base.dae"/>			
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.1"/>
			</inertial>
		</link>
		
		<!-- joint 1 -->
		<joint name="${prefix}outer_yaw" type="revolute">
			<parent link="${prefix}psm_base_link"/>
			<child link="${prefix}outer_yaw_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 -1.5708 1.5708" xyz="0.0 0.0 0.0"/>
			<dynamics damping="1"/> 
			<limit lower="-1.5707" upper="1.5707" velocity="5.0" effort="1000"/>
		</joint>

		<!-- link 1: outer_yaw -->
		<link name="${prefix}outer_yaw_link">
			<visual>				<!-- <origin rpy="0 0 -1.5708" xyz="0.0125 0 0.1575"/> -->
				<origin rpy="3.1416 0 1.5708" xyz="0.0125 0 0.5265"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/outer_yaw.dae"/>			
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial>
		</link>

		<!-- Note: I can not put visual link here as it's parallel -->
		<!-- mechanism, so the outer_pitch_link does not have visual -->
		<!-- link, all visual links mimic outer_pitch value -->
		
		<!-- joint 2: outer_pitch -->
		<joint name="${prefix}outer_pitch" type="revolute">
			<parent link="${prefix}outer_yaw_link"/>
			<child link="${prefix}outer_pitch_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="-1.5708 -1.5708 0" xyz="0 0 0"/>
			<dynamics damping="1"/>
			<limit lower="-0.7854" upper="0.7854" velocity="5.0" effort="1000"/>
		</joint>
		
		<!-- link 2: outer_pitch_link -->
		<link name="${prefix}outer_pitch_link">
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial>	
		</link>
		
		<!--wsn: for visualization links/joints of closed-chain mechanism, assign dynamic properties and control w/ fdbk-->
		<!-- joint 2-1: outer_pitch_1 -->
		<joint name="${prefix}outer_pitch_1" type="continuous">
			<parent link="${prefix}outer_yaw_link"/>
			<child link="${prefix}outer_pitch_back_link"/>
			<axis xyz="0 0 1"/>
			<!-- <origin rpy="-1.5708 -1.5708 0" xyz="0 0.0295 0.1495"/> -->
			<dynamics damping="1"/> 
			<origin rpy="-1.5708 -1.5708 0" xyz="0 0.0295 0.5185"/>
			<mimic joint="${prefix}outer_pitch" multiplier="1"/>
			<limit lower="-0.7854" upper="0.7854" velocity="5.0" effort="1000"/>
		</joint> 
		
		<!-- link 2-1: outer_pitch_back -->
		<link name="${prefix}outer_pitch_back_link">
			<visual>
				<origin rpy="0 0 -0.27129" xyz="0 0 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/outer_pitch_back.dae"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial>
		</link>

		<!-- joint 2-2: outer_pitch_2 -->
		<joint name="${prefix}outer_pitch_2" type="continuous">
			<parent link="${prefix}outer_yaw_link"/>
			<child link="${prefix}outer_pitch_front_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="-1.5708 -1.5708 0" xyz="0 0.0295 0.4285"/>
			<mimic joint="${prefix}outer_pitch" multiplier="1"/>
		</joint>

		<!-- link 2-2: outer_pitch_front -->
		<link name="${prefix}outer_pitch_front_link">
			<visual>
				<origin rpy="0 0 -0.27129" xyz="0 0 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/outer_pitch_front.dae"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial>
		</link>

		<!-- joint 2-3: outer_pitch_3 -->
		<joint name="${prefix}outer_pitch_3" type="continuous">
			<parent link="${prefix}outer_pitch_back_link"/>
			<child link="${prefix}outer_pitch_bottom_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0.04178 0.15007 -0.0137"/>
			<mimic joint="${prefix}outer_pitch" multiplier="-1"/>
			<limit lower="-0.7854" upper="0.7854" velocity="5.0" effort="1000"/>
			<dynamics damping="1"/> 
		</joint>

		<!-- link 2-3: outer_pitch_bottom -->
		<link name="${prefix}outer_pitch_bottom_link">
			<visual>
				<origin rpy="0 -1.5708 0" xyz="0.009 0 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/outer_pitch_bottom.dae"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial> 
		</link>

		<!-- joint 2-4: outer_pitch_4 -->
		<joint name="${prefix}outer_pitch_4" type="continuous">
			<parent link="${prefix}outer_pitch_back_link"/>
			<child link="${prefix}outer_pitch_top_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0.04209 0.18695 -0.02412"/>
			<mimic joint="${prefix}outer_pitch" multiplier="-1"/>
			<limit lower="-0.7854" upper="0.7854" velocity="5.0" effort="1000"/>
			<dynamics damping="1"/> 
		</joint>

		<!-- link 2-4: outer_pitch_top -->
		<link name="${prefix}outer_pitch_top_link">
			<visual>
				<origin rpy="0 -1.5708 0" xyz="0.009 0 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/outer_pitch_top.dae"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial>	
		</link>

		<!-- joint 2-5: outer_pitch_5 -->
		<joint name="${prefix}outer_pitch_5" type="continuous">
			<parent link="${prefix}outer_pitch_bottom_link"/>
			<child link="${prefix}outer_insertion_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="-0.520 0 -0.0155"/>
			<mimic joint="${prefix}outer_pitch" multiplier="1"/>
			<limit lower="-0.7854" upper="0.7854" velocity="5.0" effort="1000"/>
			<dynamics damping="1"/> 
		</joint>

		<!-- link 2-5: outer_insertion -->
		<link name="${prefix}outer_insertion_link">
			<visual>
				<origin rpy="0 -1.5708 1.5708" xyz="0.02528 0.429 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/outer_insertion.dae"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial>
		</link>
		<gazebo reference="${prefix}outer_insertion_link">
			<material>Gazebo/${color}</material>
		</gazebo>

		<!-- joint 3: insertion -->
		<joint name="${prefix}outer_insertion" type="prismatic">
			<parent link="${prefix}outer_pitch_link"/>
			<child link="${prefix}tool_main_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="1.5708 0 0" xyz="0 0.4318 0"/>
			<limit lower="0" upper="0.240" velocity="5.0" effort="1000"/>
			<dynamics damping="1"/> 
		</joint>

		<!-- link 3: tool_main_link -->
		<link name="${prefix}tool_main_link">
			<visual>
				<origin rpy="0 0 1.5708" xyz="0 0 0.041"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/tool_main.dae"/>			
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial>	
		</link>
		
		<!-- joint 4: Outer Roll -->
		<joint name="${prefix}outer_roll" type="revolute">
			<parent link="${prefix}tool_main_link"/>
			<child link="${prefix}tool_wrist_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0 0 0.4162"/>
			<limit lower="-3.0485" upper="3.0485" velocity="5.0" effort="1000"/>
			<dynamics damping="1"/>
		</joint>

		<!-- link 4: tool_wrist_link -->
		<link name="${prefix}tool_wrist_link">
			<visual>
				<origin rpy="0 0 1.5708" xyz="0 0 -0.0091"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/tool_wrist_link.dae"/>			
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial> 
		</link>

		<!-- joint 4-1: outer roll shaft joint FIXED-->
		<joint name="${prefix}outer_roll_shaft_joint" type="fixed">
			<parent link="${prefix}tool_wrist_link"/>
			<child link="${prefix}tool_wrist_shaft_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0 0 0.0"/>
		</joint>

		<!-- link 4-1: tool_wrist_shaft_link -->
		<link name="${prefix}tool_wrist_shaft_link">
			<visual>
				<origin rpy="1.5708 0 0" xyz="0 0.00401 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/tool_wrist_shaft_link.dae"/> 
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.1"/>
				<inertia
					ixx="0.1" ixy="0.0" ixz="0.0"
					iyy="0.1" iyz="0.0"
					izz="0.005"
				/>
			</inertial> 
		</link>

		<transmission name="${prefix}tran1">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="${prefix}outer_yaw">
				<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
			</joint>
			<actuator name="${prefix}motor1">
				<mechanicalReduction>1</mechanicalReduction>
			</actuator>
		</transmission>

		<transmission name="${prefix}tran2">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="${prefix}outer_pitch">
				<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
			</joint>
			<actuator name="${prefix}motor2">
				<mechanicalReduction>1</mechanicalReduction>
			</actuator>
		</transmission>
		<gazebo>
			<plugin name="mimic_1" filename="libroboticsgroup_gazebo_mimic_joint_plugin.so">
				<joint>${prefix}outer_pitch</joint>
				<mimicJoint>${prefix}outer_pitch_1</mimicJoint>
			</plugin>
			<plugin name="mimic_2" filename="libroboticsgroup_gazebo_mimic_joint_plugin.so">
				<joint>${prefix}outer_pitch</joint>
				<mimicJoint>${prefix}outer_pitch_2</mimicJoint>
			</plugin>
			<plugin name="mimic_3" filename="libroboticsgroup_gazebo_mimic_joint_plugin.so">
				<joint>${prefix}outer_pitch</joint>
				<mimicJoint>${prefix}outer_pitch_3</mimicJoint>
				<multiplier>-1.0</multiplier>
			</plugin>
			<plugin name="mimic_4" filename="libroboticsgroup_gazebo_mimic_joint_plugin.so">
				<joint>${prefix}outer_pitch</joint>
				<mimicJoint>${prefix}outer_pitch_4</mimicJoint>
				<multiplier>-1.0</multiplier>
			</plugin>
			<plugin name="mimic_5" filename="libroboticsgroup_gazebo_mimic_joint_plugin.so">
				<joint>${prefix}outer_pitch</joint>
				<mimicJoint>${prefix}outer_pitch_5</mimicJoint>
			</plugin>
		</gazebo>

	
		<transmission name="${prefix}tran3">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="${prefix}outer_insertion">
				<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
			</joint>
			<actuator name="${prefix}motor3">
				<mechanicalReduction>1</mechanicalReduction>
			</actuator>
		</transmission>
	
		<transmission name="${prefix}tran4">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="${prefix}outer_roll">
				<hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
			</joint>
			<actuator name="${prefix}motor4">
				<mechanicalReduction>1</mechanicalReduction>
			</actuator>
		</transmission>

	</xacro:macro>
</robot>
