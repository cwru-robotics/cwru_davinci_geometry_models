<?xml version="1.0" encoding="utf-8"?>
<robot name="dvrk_psm" xmlns:xacro="http://www.ros.org/wiki/xacro">

	<!--This file contains different tool urdf -->
	<xacro:include filename="$(find cwru_davinci_geometry_models)/model/common.urdf.xacro"/>

	<!--sca-->
	<xacro:macro name="psm_tool_sca" params="prefix">
		<!-- joint 5: Outer wrist pitch joint -->
		<joint name="outer_wrist_pitch" type="revolute">
			<parent link="${prefix}_tool_wrist_shaft_link"/>
			<child link="${prefix}_tool_wrist_sca_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="-1.5708 -1.5708 0" xyz="0 0 0"/>
			<limit lower="-1.5707" upper="1.5707" velocity=".4" effort="1000"/>
		</joint>
	
		<!-- link 5: tool_wrist_sca_link -->
		<link name="${prefix}_tool_wrist_sca_link">
			<visual>
				<origin rpy="1.5708 3.1416 0" xyz="0.0051 0.0032 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/tool_wrist_sca_link.dae"/> 
				</geometry>
			</visual>
			<collision>
				<origin rpy="1.5708 3.1416 0" xyz="0.0051 0 0"/>
				<geometry>
					<box size="0.011 0.007 0.005" />
				</geometry>
			</collision>
			<selfCollide>false</selfCollide>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.05"/>
				<inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.005"/>
			</inertial>
		</link>
		<gazebo reference="${prefix}_tool_wrist_sca_link">
			
		</gazebo>
		
		<!-- joint 6: Outer wrist yaw joint -->
		<joint name="outer_wrist_yaw" type="revolute">
			<parent link="${prefix}_tool_wrist_sca_link"/>
			<child link="${prefix}_tool_wrist_sca_shaft_link"/>
			<axis xyz="0 0 1"/>
			<origin rpy="-1.5708 -1.5708 0" xyz="0.0091 0 0"/>
			<!-- +/- 80 deg -->
			<limit lower="-1.3963" upper="1.3963" velocity=".4" effort="1000"/>
		</joint>

		<!-- link 6: tool_wrist_sca_shaft_link -->
		<link name="${prefix}_tool_wrist_sca_shaft_link">
			<visual>
				<origin rpy="0 0 0" xyz="0 -0.0002 -0.0025"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/tool_wrist_sca_shaft_link.dae"/> 
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.05"/>
				<inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.005"/>
			</inertial> 
		</link>
		
		<!-- joint 7-0: Outer open angle 1-->
		<joint name="jaw" type="revolute">
			<parent link="${prefix}_tool_wrist_sca_shaft_link"/>
			<child link="${prefix}_tool_wrist_sca_ee_link_0"/>			
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<limit lower="0" upper="1.5707" velocity=".4" effort="1000"/>
		</joint>

		<!-- link 7-0: Outer open angle virtual -->
		<link name="${prefix}_tool_wrist_sca_ee_link_0">
			<!--visual>
				<origin rpy="0 0 0" xyz="0 0 0"/>
				<geometry>					
					<box size="0.1 0.1 0.1" />
				</geometry>
			</visual-->
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.05"/>
				<inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.005"/>
			</inertial>
		</link>

		<!-- joint 7-1: Outer open angle 1	-->
		<joint name="jaw_mimic_1" type="revolute">
			<parent link="${prefix}_tool_wrist_sca_shaft_link"/>
			<child link="${prefix}_tool_wrist_sca_ee_link_1"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<limit lower="-1.5707" upper="1.5707" velocity=".4" effort="1000"/>
			<mimic joint="jaw" multiplier="0.5"/>
		</joint>

		<!-- link 7-1: Outer open angle link 1 -->
		<link name="${prefix}_tool_wrist_sca_ee_link_1">
			<visual>
				<origin rpy="0 0 0" xyz="0 0 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/tool_wrist_sca_link_2.dae"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.05"/>
				<inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.005"/>
			</inertial>
		</link>
		
		<!-- joint 7-2: Outer open angle 2	 -->
		<joint name="jaw_mimic_2" type="revolute">
			<parent link="${prefix}_tool_wrist_sca_shaft_link"/>
			<child link="${prefix}_tool_wrist_sca_ee_link_2"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<limit lower="-1.5707" upper="1.5707" velocity=".4" effort="1000"/>
			<mimic joint="jaw" multiplier="-0.5"/>
		</joint>

		<!-- link 7-2: Outer open angle link 2 -->
		<link name="${prefix}_tool_wrist_sca_ee_link_2">
			<visual>
				<origin rpy="0 3.1516 0" xyz="0 0 0"/>
				<geometry>
					<mesh filename="package://cwru_davinci_geometry_models/meshes/psm/tool_wrist_sca_link_2.dae"/>
				</geometry>
			</visual>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.05"/>
				<inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.005"/>
			</inertial> 
		</link>
		
		<link name="${prefix}_fingertip2">
			<visual>
				<origin rpy="0 0 0" xyz="0.001 0.005 0.0005"/>
				<geometry>
					<box size="0.002 0.01 0.004"/>
				</geometry>
			</visual>
			<collision>
				<origin rpy="0 0 0" xyz="0.001 0.005 0.0005"/>
				<geometry>
					<box size="0.002 0.01 0.004"/>
				</geometry>
				<surface>
					<friction>
						<ode>
							<mu>30.0</mu>
							<mu2>30.0</mu2>
						</ode>
					</friction>
					<contact>
						<ode>
							<kp>1000000.0</kp>
							<kd>100.0</kd>
							<max_vel>1.0</max_vel>
							<min_depth>0.001</min_depth>
						</ode>
					</contact>
				</surface>
			</collision>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.05"/>
				<inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.005"/>
			</inertial> 
		</link>
		<!-- attach fingertip pad  -->
		<joint name="${prefix}_fingertip2_joint" type="fixed">
			<parent link="${prefix}_tool_wrist_sca_ee_link_2"/>
			<child link="${prefix}_fingertip2"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
		</joint>
	
		<!-- wsn: other fingertip -->
		<link name="${prefix}_fingertip1">
			<visual>
				<origin rpy="0 0 0" xyz="-0.001 0.005 0.0005"/>
				<geometry>
					<box size="0.002 0.01 0.004"/>
				</geometry>
			</visual>
			<collision name="${prefix}_fingertip1_collision">
				<origin rpy="0 0 0" xyz="-0.001 0.005 0.0005"/>
				<geometry>
					<box size="0.002 0.01 0.004"/>
				</geometry>
				<surface>
					<friction>
						<ode>
							<mu>30.0</mu>
							<mu2>30.0</mu2>
						</ode>
					</friction>
					<contact>
						<ode>
							<kp>1000000.0</kp>
							<kd>100.0</kd>
							<max_vel>1.0</max_vel>
							<min_depth>0.001</min_depth>
						</ode>
					</contact>
				</surface>
			</collision>
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="0.05"/>
				<inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.005"/>
			</inertial>
		</link>
	
		<!-- attach fingertip pad 1  -->
		<joint name="${prefix}_fingertip1_joint" type="fixed">
			<parent link="${prefix}_tool_wrist_sca_ee_link_1"/>
			<child link="${prefix}_fingertip1"/>
			<axis xyz="0 0 1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
		</joint>
		

		<!-- tool tip -->
		<joint name="${prefix}_tool_tip_joint_" type="fixed">
			<parent link="${prefix}_tool_wrist_sca_shaft_link"/>
			<child link="${prefix}_tool_tip_link"/>
			<origin rpy="0 ${PI/2} ${PI/2}" xyz="0 0.0102 0"/>
		</joint>
	    
		<link name="${prefix}_tool_tip_link">
			<visual>
				<geometry>
					<!-- put a fiducial, a sphere, at the tool tips -->
					<sphere radius="0.001"/>
				</geometry>
	
			</visual>
		</link>

		<!--this colors the tool-tip fiducial red; oddly, this shows up in the camera view, but NOT in the rviz view -->
		<gazebo reference="${prefix}_tool_tip_link">
			<material>Gazebo/Red</material>
		</gazebo>
		
		<!--Color the sticky fingertips pink, for visibility-->
		<gazebo reference="${prefix}_fingertip1">
			<material>Gazebo/Purple</material>
		</gazebo>
		
		<!--Create the sticky fingertip.-->
		<gazebo>
			<plugin name="${prefix}_fingertip1_finger" filename="libsticky_fingers.so">
				<capacity>25</capacity>
				<link>davinci_${prefix}::${prefix}_tool_wrist_sca_ee_link_1</link>
			</plugin>
         	</gazebo>
		
		<transmission name="${prefix}_tran5">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="outer_wrist_pitch">
				<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
			</joint>
			<actuator name="${prefix}_motor5">
				<mechanicalReduction>1</mechanicalReduction>
			</actuator>
		</transmission>
	
		<transmission name="${prefix}_tran6">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="outer_wrist_yaw">
				<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
			</joint>
			<actuator name="${prefix}_motor6">
				<mechanicalReduction>1</mechanicalReduction>
			</actuator>
		</transmission>
		
		<transmission name="${prefix}_tran7">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="jaw">
				<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
			</joint>
			<actuator name="${prefix}_motor7">
				<mechanicalReduction>1</mechanicalReduction>
			</actuator>
		</transmission>
		<gazebo>
			<plugin name="mimic_6" filename="libroboticsgroup_gazebo_mimic_joint_plugin.so">
				<joint>jaw</joint>
				<mimicJoint>jaw_mimic_1</mimicJoint>
				<multiplier>0.5</multiplier>
			</plugin>
			<plugin name="mimic_7" filename="libroboticsgroup_gazebo_mimic_joint_plugin.so">
				<joint>jaw</joint>
				<mimicJoint>jaw_mimic_2</mimicJoint>
				<multiplier>-0.5</multiplier>
			</plugin>
		</gazebo>
	</xacro:macro>
	
</robot>
